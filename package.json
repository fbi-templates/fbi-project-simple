{
  "name": "fbi-project-simple",
  "version": "1.1.0",
  "description": "Simple web application using parcel",
  "engines": {
    "node": ">= 7.6.0",
    "npm": ">= 3.10.0"
  },
  "scripts": {},
  "author": "neikvon",
  "license": "MIT",
  "devDependencies": {
    "@vue/component-compiler-utils": "3.0.0",
    "babel-preset-hyperapp": "2.3.0",
    "babel-preset-react": "6.24.1",
    "babel-preset-vue": "2.0.2",
    "express": "4.17.1",
    "parcel-bundler": "1.12.3",
    "vue-template-compiler": "2.6.10"
  },
  "fbi": {
    "template": "fbi-project-simple",
    "tasks": {
      "build": {
        "alias": "b",
        "desc": "Build the project for the production environment."
      },
      "serve": {
        "alias": "s",
        "desc": "Start development server."
      }
    }
  }
}
